                },
                {
                  "color": "red",
                  "value": 80
                }
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byRegexp",
                "options": "/.*/"
              },
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {
                      "options": {
                        "Fail": {
                          "color": "red",
                          "index": 1,
                          "text": "Fail"
                        },
                        "Pass": {
                          "color": "green",
                          "index": 0,
                          "text": "Pass"
                        }
                      },
                      "type": "value"
                    }
                  ]
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "compliance_id\\cluster_id"
              },
              "properties": [
                {
                  "id": "mappings",
                  "value": [
                    {
                      "options": {
                        "pattern": "/.*/",
                        "result": {
                          "color": "transparent",
                          "index": 0
                        }
                      },
                      "type": "regex"
                    }
                  ]
                },
